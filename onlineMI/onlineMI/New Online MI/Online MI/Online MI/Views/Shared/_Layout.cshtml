<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>Online MI</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport' />
    @Styles.Render("~/Content/css")
    <script src="~/Scripts/angular.min.js" type="text/javascript"></script>
    <script src="~/Scripts/angular-animate.js"></script>
    <script src="~/Scripts/angular-sanitize.js"></script>
    <link href="//cdn.datatables.net/1.10.4/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link rel="shortcut icon" href="https://connex.nyscorporate.com/images/favicon.ico" type="image/x-icon" />
</head>
<body class="skin-Online_MI" ng-app="app" ng-controller="DashboardCtrl">
    <header class="header">
        <a href="#" class="logo">
            Client MI
        </a>
        <!-- Header Navbar: style can be found in header.less -->
        <nav class="navbar navbar-static-top" role="navigation">
            <!-- Sidebar toggle button-->
            <a href="#" class="navbar-btn sidebar-toggle" data-toggle="offcanvas" role="button">
                <span class="sr-only">Toggle navigation</span>
                <img src="~/Content/Images/MenuIcon.png" />
            </a>
            <div class="navbar-right">
                <ul class="nav navbar-nav">
                    <!-- User Account: style can be found in dropdown.less -->
                    <li class="dropdown user user-menu">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            <i class="fa fa-user"></i>
                        </a>
                        <ul class="dropdown-menu">
                            <!-- User image -->
                            <li class="user-header bg-light-blue">

                            </li>
                            <!-- Menu Footer-->
                            <li class="user-footer">
                                <div class="pull-right">
                                    <a href="/Home/Logout" class="btn btn-default btn-flat">Sign out</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <div class="wrapper row-offcanvas row-offcanvas-left">
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="left-side sidebar-offcanvas">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li>
                        <a href="/Home">
                            <i class="fa fa-home"></i> Dashboard
                        </a>
                    </li>
                    <li>
                        <a href="#" ng-click="getRail()">
                            <i class="fa fa-ticket"></i> Trains
                        </a>
                    </li>
                    <li>
                        <a href="#" ng-click="getFlights()">
                            <i class="fa fa-plane"></i> Flights
                        </a>
                    </li>
                    <li>
                        <a href="#" ng-click="getAccommodation()">
                            <i class="fa fa-home"></i> Accommodation
                        </a>
                    </li>
                    <li>
                        <a href="#" ng-click="getConferences()">
                            <i class="fa fa-users"></i> Conference
                        </a>
                    </li>
                    <li>
                        <a href="#" ng-click="getComplaints()">
                            <i class="fa fa-file"></i> Complaints
                        </a>
                    </li>
                    <li>
                        <a href="#" ng-click="getFeedback()">
                            <i class="fa fa-file"></i> Feedback
                        </a>
                    </li>
                    <li>
                        <a href="#" ng-click="getTravellerTracking()">
                            <i class="fa fa-users"></i> Traveller Tracking
                        </a>
                    </li>
                    <li>
                        <a href="/Home/Logout">
                            <i class="fa fa-lock"></i> Logout
                        </a>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Right side column. Contains the navbar and content of the page -->
        <aside class="right-side">
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section><!-- /.content -->
        </aside><!-- /.right-side -->
    </div><!-- ./wrapper -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/AdminLTE")
    @Scripts.Render("~/bundles/custom")
    @Scripts.Render("~/bundles/CustomAngular")
    <script src="~/Scripts/smart-table.min.js"></script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-54715771-2', 'auto');
        ga('send', 'pageview');

    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
